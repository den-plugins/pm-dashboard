<table id="pm_dashboard_header">
<div id='dashboard_header'>
  <tr>
    <td><span>Client</span></td><td><%= @project.client %></td>
    <td><span>Planned Start Date</span></td><td><%= @project.planned_start_date.to_s %></td>
    <td><span>Maintenance End Date</span></td><td><%= @project.maintenance_end.to_s %></td>
    <td><span>Technical Architect</span></td><td><%= @project.pm_or_ta(@project.tech_architect, @project) %></td>
  </tr>
  
  <tr>
    <td><span>Project Name</span></td><td><%= @project.name %></td>    
    <td><span>Planned End Date</span></td><td><%= @project.planned_end_date.to_s %></td>    
    <td><span>Project Manager</span></td><td><%= @project.pm_or_ta(@project.proj_manager, @project) %></td>    
    <td><span>Account Manager</span></td><td> </td>    
  </tr>   
</div>
  <tr>
    <td><span>Overall Status</span></td><td><%=  %></td>
    <td><span>Reporting Period</span></td><td><%= @highlights[:current].created_at.strftime("%Y-%m-%d") if @highlights[:current] %></td>
    <td><span>Overall % Complete</span></td><td><%=  %></td>
  </tr>
  
  <tr>
    <td><span>Schedule</span></td><td><%= %></td>
    <td><span>Cost</span></td><td><%= %></td>
    <td><span>Risk</span></td><td><%= %></td>
    <td><span>Issue</span></td><td><%= %></td>
    <td><span>Production</span></td><td><%= %></td>
    <td><span>Efficiency</span></td><td><%= %></td>
    <td><span>Billability</span></td><td><%= %></td>
  </tr>
</table>
  
  <div class="singleCol">
    <div id='highlights_summary'>
      <%= render :partial => 'pm_dashboards/highlights/dashboard' %>
    </div>
  </div>
    
  <div class="singleCol">
    <div class="box">
      <h3><%=l(:label_dashboard_status_as_of)%></h3>
      <span id="dashboard_cost_monitoring">
        <%= render :partial => "pm_dashboards/project_billability/cost_monitoring_dashboard" %>
      </span>
    </div>

    <div class="box">
      <h3><%=l(:label_dashboard_risks)%></h3> 
      <ul>
      <% for risk in @key_risks %>    
        <li><%= risk.risk_description %></li>
      <% end %>
      </ul>
    </div>
      
    <div class="box">
      <h3><%=l(:label_dashboard_billability)%></h3>
      <span id="dashboard_billability">
        <%= render :partial => "pm_dashboards/project_billability/billability_dashboard" %>
      </span>
    </div>
  </div>
  
  <div class="singleCol">
    <div class="box">
      <h3><%=l(:label_dashboard_milestones)%></h3>
       <table id="pm_dashboard_milestones" class="list issues" style='text-align: center; '>
	      <tr>
	        <th><%= l(:label_actual_start_date) %></th>
	        <td></td>
	        <th>Schedule End Date</th>
	        <td></td>
	      </tr>
	      <tr>
	        <th><%= l(:label_phases_stages) %></th>
	        <th><%= l(:label_orig_target_date) %></th>
	        <th><%= l(:label_latest_replan_date) %></th>
	        <th><%= l(:label_schedule_status) %></th>
	      </tr>
		    <tr>
	        <td></td>
	        <td></td>
	        <td></td>
	        <td></td>
	      </tr>
      </table>
    </div>

    <div class="box">
      <h3><%=l(:label_dashboard_issues)%></h3>
      <ul>
      <% for issue in @key_issues %>    
        <li><%= issue.issue_description %></li>
      <% end %>
      </ul>
    </div>
    
    <div class="box">
      <h3><%=l(:label_dashboard_sprint)%></h3>
    </div>
  </div>

