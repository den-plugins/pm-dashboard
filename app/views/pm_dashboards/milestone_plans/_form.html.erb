<div>
	<div>
		<%= f.label :name %><br>
		<%= f.text_field :name %>
	</div>
  <div>
    <p>
    Original Target Date
    <%= f.text_field :original_target_date, :size => 10 %><%= calendar_for('milestone_original_target_date') %></p>
    <p>
    Latest Re-Plan Date
    <%= f.text_field :latest_re_plan_date, :size => 10 %><%= calendar_for('milestone_latest_re_plan_date') %></p>
  </div>

	<div class="field">
		<%= f.label :remarks %><br>
		<%= f.text_area :remarks %>
	</div>
	<div class="field">
		<%= f.label :status %><br>
		<%= f.text_field :status %>
	</div>
	<div>
		<%= f.submit button_label %>
		<%= link_to "Cancel", :controller => "pm_dashboards", :project_id => @project, :tab => "milestone_plans" %>
	</div>
</div>